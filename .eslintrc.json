{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "class-methods-use-this": "off",
      "eqeqeq": "off",
      "camelcase": "off",
      "prefer-arrow-callback": "off",
    }
}
